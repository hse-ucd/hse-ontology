@prefix phi: <https://oahp.hse-data.ie/Indicators#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Base definitions
phi:Indicator a owl:Class ;
    rdfs:label "Indicator" ;
    rdfs:comment "A population health indicator that measures specific aspects of health and wellbeing." ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

# Properties with appropriate ranges
phi:indicatorTitle a owl:DatatypeProperty ;
    rdfs:label "indicator title" ;
    rdfs:comment "Full name of the indicator as used in reports or dashboards." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:indicatorID a owl:DatatypeProperty ;
    rdfs:label "indicator ID" ;
    rdfs:comment "The main identifier for the Indicator, e.g. the URI -URL, web link- or other unique identifier in the context of the Catalogue.
" ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:anyURI ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:rationale a owl:DatatypeProperty ;
    rdfs:label "rationale" ;
    rdfs:comment "A summary explaining why this indicator matters—its relevance to population health, what it helps to understand, and how it complements other indicators.
" ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:definition a owl:DatatypeProperty ;
    rdfs:label "definition" ;
    rdfs:comment "Technical description of the indicator—what it measures, how it's defined, and any assumptions or limitations in interpretation." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:indicatorType a owl:ObjectProperty ;
    rdfs:label "indicator type" ;
    rdfs:comment "Specifies if the indicatorhas both a numerator and a denominator, is coverage, accumulated or qualitative type
" ;
    rdfs:domain phi:Indicator ;
    rdfs:range skos:Concept ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:status a owl:ObjectProperty ;
    rdfs:label "status" ;
    rdfs:comment "Internal prioritisation process status for the dataset or indicator." ;
    rdfs:domain phi:Indicator ;
    rdfs:range skos:Concept ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:disaggregations a owl:DatatypeProperty ;
    rdfs:label "disaggregations" ;
    rdfs:comment "A list of disaggregation conditions that should be available e.g. male and female." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:numeratorDataElement a owl:DatatypeProperty ;
    rdfs:label "numerator data element" ;
    rdfs:comment "The description of one variable or value that is used in the numerator e.g. number of deaths. Multiple data elements may be present as a list." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:numeratorSourceDataset a owl:ObjectProperty ;
    rdfs:label "numerator source dataset" ;
    rdfs:comment "Identifier, e.g. URL, for the dataset providing a numerator data element. If there are multiple numerator data elements then multiple datasets must be provided as a list." ;
    rdfs:domain phi:Indicator ;
    rdfs:range dcat:Dataset ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:denominatorDataElement a owl:DatatypeProperty ;
    rdfs:label "denominator data element" ;
    rdfs:comment "The description of one variable or value that is used in the denominator (if this indicator has one) e.g. population size. Multiple data elements may be present as a list." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:denominatorSourceDataset a owl:ObjectProperty ;
    rdfs:label "denominator source dataset" ;
    rdfs:comment "Identifier, e.g. URL, for the dataset providing a denominator data element. If there are multiple denominator data elements then multiple datasets must be provided as a list." ;
    rdfs:domain phi:Indicator ;
    rdfs:range dcat:Dataset ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:methodology a owl:DatatypeProperty ;
    rdfs:label "methodology" ;
    rdfs:comment "Summary of methods used to calculate the indicator -e.g., statistical technique, smoothing, standardisation, modelling-." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:reportFrequency a owl:ObjectProperty ;
    rdfs:label "report frequency" ;
    rdfs:comment "Frequency of reporting period, e.g. daily or monthly. This is used with the reporting style to determine the exact frequency and lag of reports." ;
    rdfs:domain phi:Indicator ;
    rdfs:range skos:Concept ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:reportStyle a owl:ObjectProperty ;
    rdfs:label "report style" ;
    rdfs:comment "Indicates the period to which the data applies and how that is used with reporting frequency to generate a report." ;
    rdfs:domain phi:Indicator ;
    rdfs:range skos:Concept ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:domain a owl:ObjectProperty ;
    rdfs:label "domain" ;
    rdfs:comment "A high-level category the indicator falls under -e.g. Demography-." ;
    rdfs:domain phi:Indicator ;
    rdfs:range skos:Concept ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:subdomain a owl:ObjectProperty ;
    rdfs:label "subdomain" ;
    rdfs:comment "A more specific category under the domain -e.g. Population numbers-." ;
    rdfs:domain phi:Indicator ;
    rdfs:range skos:Concept ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:highLowGuidance a owl:DatatypeProperty ;
    rdfs:label "high-low guidance" ;
    rdfs:comment "Guidance on how to understand high or low values—whether they suggest good or poor outcomes." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:measurementLimitations a owl:DatatypeProperty ;
    rdfs:label "measurement limitations" ;
    rdfs:comment "Notes on limitations or uncertainty introduced by how the data are collected or calculated." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:validityGuidance a owl:DatatypeProperty ;
    rdfs:label "validity guidance" ;
    rdfs:comment "Description of how the indicator might be influenced by other variables or biases, affecting validity." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:publicHealthImportance a owl:DatatypeProperty ;
    rdfs:label "public health importance" ;
    rdfs:comment "Explanation of the significance of this indicator in terms of population health impact, burden of disease, or priority within health strategy." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:provenance a owl:DatatypeProperty ;
    rdfs:label "provenance" ;
    rdfs:comment "Description of source of this indicator, e.g. academic papers, standards." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:notes a owl:DatatypeProperty ;
    rdfs:label "notes" ;
    rdfs:comment "Any additional informal information about the dataset or indicator." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

phi:reasonsForInclusionExclusion a owl:DatatypeProperty ;
    rdfs:label "reasons for inclusion/exclusion" ;
    rdfs:comment "Reasons for including or excluding the indicator from the catalogue." ;
    rdfs:domain phi:Indicator ;
    rdfs:range xsd:string ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:modified "2025-08-22"^^xsd:date .

# Ontology definition
phi: a owl:Ontology ;
    rdfs:label "Population Health Indicator Ontology" ;
    rdfs:comment "An ontology for describing population health indicators, created during the Older Adult Health and Wellbeing Profile project." ;
    dct:title "Population Health Indicator Ontology" ;
    dct:description "Ontology for modeling population health indicators based on health DCAT-AP data catalogue" ;
    
    # Version information
    owl:versionInfo "1.0" ;
    owl:versionIRI <https://github.com/hse-ucd/hse-ontology/blob/main/phiontology.ttl> ;
    dct:issued "2025-08-20"^^xsd:date ;
    dct:modified "2025-08-21"^^xsd:date ;
    
    # Contributors
    dct:contributor <https://oahp.hse-data.ie/people#RobBrennan> ;
    dct:contributor <https://oahp.hse-data.ie/people#JunliLiang> ;
    dct:contributor <https://oahp.hse-data.ie/people#HaulaGaladima> ;
    
    # Source and relations
    dct:source "OAHP_DataCatalogueSchemaProposal-v05.12.xlsx" ;
    rdfs:seeAlso <https://hse-ucd.github.io/hse-ontology/#https://oahp.hse-data.ie/Indicators#> ;
    rdfs:seeAlso <https://openreview.net/pdf?id=JMWarUh56o> .