@prefix : <https://oahp.hse-data.ie/Indicators#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:IndicatorTitle a owl:Class ;
    rdfs:label "IndicatorTitle" ;
    rdfs:comment "Full name of the indicator as used in reports or dashboards." ;
    rdfs:seeAlso :IndicatorID, :Definition, :Rationale ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:IndicatorID a owl:Class ;
    rdfs:label "IndicatorID" ;
    rdfs:comment "The main identifier for the Indicator, e.g. the URI [URL, web link] or other unique identifier in the context of the Catalogue." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:Rationale a owl:Class ;
    rdfs:label "Rationale" ;
    rdfs:comment "A summary explaining why this indicator matters—its relevance to population health, what it helps to understand, and how it complements other indicators." ;
    rdfs:seeAlso :IndicatorTitle, :Definition, :PublicHealthImportance ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:Definition a owl:Class ;
    rdfs:label "Definition" ;
    rdfs:comment "Technical description of the indicator—what it measures, how it's defined, and any assumptions or limitations in interpretation." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:IndicatorType a owl:Class ;
    rdfs:label "IndicatorType" ;
    rdfs:comment "Specifies if the indicator has both a numerator and a denominator, is coverage, accumulated or qualitative type" ;
    rdfs:seeAlso :Status, :Domain, :Subdomain ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:Status a owl:Class ;
    rdfs:label "Status" ;
    rdfs:comment "An internal prioritisation process status for the dataset or indicator" ;
    rdfs:seeAlso :IndicatorID, :IndicatorType ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:Disaggregations a owl:Class ;
    rdfs:label "Disaggregations" ;
    rdfs:comment "A list of disaggregation conditions that should be available e.g. male and female" ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:NumeratorDataElement a owl:Class ;
    rdfs:label "NumeratorDataElement" ;
    rdfs:comment "The description of one variable or value that is used in the numerator e.g. number of deaths. Multiple data elements may be present as a list." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:NumeratorSourceDataset a owl:Class ;
    rdfs:label "NumeratorSourceDataset" ;
    rdfs:comment "Identifier, e.g. URL, for the dataset providing a numerator data element. If there are multiple numerator data elements then multiple datasets must be provided as a list." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:DenominatorDataElement a owl:Class ;
    rdfs:label "DenominatorDataElement" ;
    rdfs:comment "The description of one variable or value that is used in the denominator (if this indicator has one) e.g. population size. Multiple data elements may be present as a list." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:DenominatorSourceDataset a owl:Class ;
    rdfs:label "DenominatorSourceDataset" ;
    rdfs:comment "Identifier, e.g. URL, for the dataset providing a denominator data element. If there are multiple denominator data elements then multiple datasets must be provided as a list." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:Methodology a owl:Class ;
    rdfs:label "Methodology" ;
    rdfs:comment "Summary of methods used to calculate the indicator (e.g., statistical technique, smoothing, standardisation, modelling)." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:ReportFrequency a owl:Class ;
    rdfs:label "ReportFrequency" ;
    rdfs:comment "Frequency of reporting period, e.g. daily or monthly. This is used with the reporting style to determine the exact frequency and lag of reports." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:ReportStyle a owl:Class ;
    rdfs:label "ReportStyle" ;
    rdfs:comment "Indicates the period to which the data applies and how that is used with reporting frequency to generate a report." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:Domain a owl:Class ;
    rdfs:label "Domain" ;
    rdfs:comment "The high-level category the indicator falls under (e.g. Demography)." ;
    rdfs:seeAlso :Subdomain, :IndicatorType ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:Subdomain a owl:Class ;
    rdfs:label "Subdomain" ;
    rdfs:comment "A more specific category under the domain (e.g. Population numbers)." ;
    rdfs:seeAlso :Domain, :IndicatorType ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:HighLowGuidance a owl:Class ;
    rdfs:label "HighLowGuidance" ;
    rdfs:comment "Guidance on how to understand high or low values—whether they suggest good or poor outcomes." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:MeasurementLimitations a owl:Class ;
    rdfs:label "MeasurementLimitations" ;
    rdfs:comment "Notes on limitations or uncertainty introduced by how the data are collected or calculated." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:ValidityGuidance a owl:Class ;
    rdfs:label "ValidityGuidance" ;
    rdfs:comment "Description of how the indicator might be influenced by other variables or biases, affecting validity." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:PublicHealthImportance a owl:Class ;
    rdfs:label "PublicHealthImportance" ;
    rdfs:comment "Explanation of the significance of this indicator in terms of population health impact, burden of disease, or priority within health strategy." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:Provenance a owl:Class ;
    rdfs:label "Provenance" ;
    rdfs:comment "Description of source of this indicator, e.g. academic papers, standards." ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:Notes a owl:Class ;
    rdfs:label "Notes" ;
    rdfs:comment "Any additional informal information about the dataset or indicator" ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:ReasonsForInclusionExclusion a owl:Class ;
    rdfs:label "ReasonsForInclusionExclusion" ;
    rdfs:comment "Reasons for Inclusion/Exclusion" ;
    rdfs:seeAlso :IndicatorTitle ;
    dct:source "OAHP Data Catalogue Schema Proposal v05.12" ;
    dct:creator "Rob" ;
    dct:created "2025-08-18"^^xsd:date ;
    dct:isPartOf :OAHP_Indicators .

:OAHP_Indicators a owl:Ontology ;
    rdfs:label "OAHP Indicators" .
